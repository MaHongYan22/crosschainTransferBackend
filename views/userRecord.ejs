                </div>
            </div>
            <thead>
                <tr>
                    <th><input type="checkbox" class="checkAll"></th>
                    <th>交易Id</th>
                    <th>交易状态</th>
                    <th>交易类型</th>
                    <th>交易参与链ID</th>
                    <th>创建时间</th>
                    <th width="90">操作</th>
                </tr>
            </thead>
            <tbody>

            </tbody>
        </table>
        <ul class="pagebox">
            <li>首页</li>
            <li>上一页</li>
            <li class="current">1</li>
            <li>2</li>
            <li>3</li>
